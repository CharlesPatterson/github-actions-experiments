  "description": "This is a personal repo for testing out some github actions",
  "main": "dist/cli.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --loglevel warn --write \"**/*.{ts,js,json,yaml}\"",
