  "dependencies": {
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.8",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  }
